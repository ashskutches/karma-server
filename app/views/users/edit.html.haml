= render :partial => "/shared/header", :locals => { :title => "Edit User", :subtitle => @user.permalink, :flash => flash }

- form_for @user do |f|
  = render :partial => "form", :locals => { :f => f }
  
  %button{:type => "submit", :name => "commit", :value => "Update", :class => 'button positive'} Update User
  